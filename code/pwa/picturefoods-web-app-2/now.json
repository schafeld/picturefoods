{
  "version": 2,
  "name": "picturefoods",
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "manifest.js", "dest": "/manifest.js" },
    { "src": "index.html", "dest": "/index.html" },
    {"src": ".*", "dest": "/index.html"}
  ]
}