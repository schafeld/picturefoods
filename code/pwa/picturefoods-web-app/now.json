{
  "version": 2,
  "name": "picturefoods",
  "alias": ["www.picturefoods.com", "app.picturefoods.com", "picturefoods.com"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "manifest.js", "dest": "/manifest.js" },
    { "src": ".html", "dest": "/index.html" }
  ]
}